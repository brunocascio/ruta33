			<img width="163" height="162" src="{{ asset('img/logo_ruta33.png') }}" alt="Ruta 33" title="Ruta 33" /> 
		</div>
	{% endblock %}
	{% block javascripts %}
		<script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"></script>
